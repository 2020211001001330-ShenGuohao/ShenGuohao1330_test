<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

       <servlet>
           <servlet-name>helloServlet</servlet-name>
           <servlet-class>com.ShenGuohao.week2.HelloServlet</servlet-class>
       </servlet>
     <servlet-mapping>
         <servlet-name>helloServlet</servlet-name>
         <url-pattern>/hello</url-pattern>
     </servlet-mapping>
      
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>com.ShenGuohao.week3.RegisterServlet</servlet-class>
        <load-on-startup>2</load-on-startup>

    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/hhhhh</url-pattern>
    </servlet-mapping> 
    
     
    <servlet>
        <servlet-name>JdbcServlet</servlet-name>
        <servlet-class>com.ShenGuohao.week4.JdbcServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>JdbcServlet</servlet-name>
        <url-pattern>/jdbc</url-pattern>
    </servlet-mapping>
    
</web-app>